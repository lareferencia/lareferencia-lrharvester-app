<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:flowable="http://flowable.org/bpmn"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             targetNamespace="http://lareferencia.org/flowable"
             xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL
                                 http://www.omg.org/spec/BPMN/2.0/20100501/BPMN20.xsd">

  <process id="networkClean" name="Network Clean" flowable:order="60" isExecutable="true">
    <documentation>Clean old snapshots from a network (v2)</documentation>
    
    <startEvent id="start" name="Start"/>
    
    <serviceTask id="cleanTask" 
                 name="Clean Snapshots"
                 flowable:delegateExpression="${workerDelegateFactory.create('networkCleanWorker')}"/>
    
    <endEvent id="end" name="End"/>
    
    <sequenceFlow id="flow1" sourceRef="start" targetRef="cleanTask"/>
    <sequenceFlow id="flow2" sourceRef="cleanTask" targetRef="end"/>
  </process>

</definitions>
